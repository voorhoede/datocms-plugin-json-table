{
  "name": "datocms-plugin-json-table",
  "homepage": "https://github.com/voorhoede/datocms-plugin-json-table",
  "version": "2.0.1",
  "description": "A plugin for DatoCMS that makes it possible to add/insert a key and value and output a simple JSON table",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "cross-env BROWSER='none' PUBLIC_URL='/' react-scripts start",
    "build": "cross-env PUBLIC_URL='.' react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "datocms",
    "datocms-plugin",
    "field-editor",
    "json-field",
    "key-value-pair",
    "json-table",
    "json"
  ],
  "files": [
    "build",
    "docs"
  ],
  "author": "Voorhoede",
  "license": "MIT",
  "datoCmsPlugin": {
    "title": "JSON Table",
    "previewImage": "docs/preview.gif",
    "coverImage": "docs/cover.jpg",
    "entryPoint": "build/index.html"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  },
  "dependencies": {
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.14",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "datocms-plugin-sdk": "^0.5.2",
    "datocms-react-ui": "^0.5.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "typescript": "^4.5.5"
  }
}
